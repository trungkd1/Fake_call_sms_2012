<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:background="@drawable/receive1">
	<LinearLayout android:layout_width="match_parent" android:orientation="vertical"
		android:layout_height="0dip" android:layout_weight="1">
		
			
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="wrap_content" android:layout_gravity="center_horizontal"
		android:gravity="center_horizontal" android:orientation="vertical">
		<TextView android:layout_height="wrap_content" android:text="Voice Call"
			android:id="@+id/textView1" android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_marginBottom="5dip"
			android:layout_width="match_parent" android:gravity="center_horizontal"></TextView>
		<ImageView android:layout_height="@dimen/icon_size" android:id="@+id/imageView1"
			android:src="@drawable/ic_no_photo" android:layout_width="@dimen/icon_size"
			android:layout_gravity="center_horizontal" android:scaleType="fitCenter"/>
			
			<TextView android:layout_width="fill_parent"
				android:textSize="30dip" android:layout_height="wrap_content"
				android:gravity="center" android:id="@+id/textName" android:text="y"/>

			<TextView android:layout_width="fill_parent"
				android:textSize="30dip" android:layout_height="wrap_content"
				android:gravity="center" android:id="@+id/textNumber" android:text="x"/>
	</LinearLayout>
		<LinearLayout android:orientation="vertical" android:layout_weight="0.35" android:gravity="bottom"
			android:layout_width="match_parent" android:layout_height="0dip" android:layout_marginBottom="15dip" >
		
				<TextView android:layout_width="fill_parent" android:gravity="right" android:layout_marginRight="50dip"
				android:textSize="20dip" android:layout_height="wrap_content" 
				android:text="abc" android:id="@+id/textCount" />
			
		</LinearLayout>
	</LinearLayout>
	
	
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="0dip" android:layout_weight="0.52">
		<ImageView 
			android:layout_width="0dip" android:layout_height="85dip"
			android:layout_weight="1" />
		<ImageView android:layout_width="0dip"
			android:layout_height="85dip" android:layout_weight="1" />

		<ImageView android:id="@+id/imageEndcallRecieve"
			android:onClick="onClick"
			android:layout_width="0dip" android:layout_height="85dip"
			android:layout_weight="1" />
		
	</LinearLayout>
</LinearLayout>
